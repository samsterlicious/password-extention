{
  "manifest_version": 3,
  "name": "Passwordz",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "angular/dist/angular/index.html",
    "default_icon": "pw.png"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["out/scripts/content.js"],
      "matches": [
        "*"
      ]
    }
  ],
  "permissions": ["activeTab"]
}
